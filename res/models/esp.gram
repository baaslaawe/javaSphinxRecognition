grammar esp;

public <speechSet> = <basicCommands> | <control> <objects> | <exit>;
<basicCommands> = (anula | ok | elimina | modifica | mueve);
<control> = (nueva | nuevo);
<objects> = (cilindros | cubo | linea | esfera);
<exit> = (termina control vocal);
